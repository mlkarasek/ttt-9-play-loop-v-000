#!/usr/bin/env ruby

require_relative "../lib/play"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"
display_board(board)
display_board(play)
# Make sure to execute #play below
def play(board)
  turn = 0
while turn < 9
  puts "Last move!"
  turn(board)
  turn += 1
end
end
